<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Flipkart">
   <parameter name="selenium.host" value="192.168.0.103" /> <parameter name="selenium.port" 
		value="4444" /> 
	<listeners>
	<listener class-name="com.Flipkart.Utility.CustomReporter" />
	<listener class-name="com.Flipkart.Utility.CustomReporterBuildSummary" />
</listeners>
	<test name="Search Request ID">
		<classes>
			<class name="com.Flipkart.AddtoCart.AddtoCartIndex">
				<methods>
					<include name="AddToCart"></include>
					<include name="Search"></include>
				</methods> 
			</class>
		</classes>
	</test>
	</suite>